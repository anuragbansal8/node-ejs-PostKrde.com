<%include head%>
<%include navigator%>

 <div id="page">
		<div id="content">
			<div class="box">
<h2>Ads Category : <%= scatnm.scnm %> </h2>
<h3>Total Ads : <%= data.length %></h3>

<% for(i=0;i<data.length;i++) { %>

<table style='border:3px solid gray; border-radius:10px;' height='130' width='500'>
<tr>
<td width='40%' rowspan='3'>
<center>
<img src='../uploads/<%= data[i].image1 %>' height='100' width='150' />
</center>
</td>
<td><b>Title : <%= data[i].title %></b></td> 	
</tr>
<tr>
<td><b>Price : &#8377;<%= data[i].price %></b></td>
</tr>
<tr>
<td><b>City : <%= data[i].city %></b></td>
</tr>
</table>
<br><br>
<% } %>

			</div>
			
			<br class="clearfix" />
		</div>
            
		<div id="sidebar">
			<div class="box">
<h3>Ads Filter</h3>
Filter By City :
<br><br>		
<form>

<input type='hidden' name='scnm' value='<%= scatnm.scnm %>' />

<select name='city'>
<option>Select city</option>
<option>Indore</option>
<option>Bhopal</option>
<option>Ujjain</option>
</select>

&nbsp;&nbsp;&nbsp;&nbsp;
<input type='submit' value='Search' />

</form>


</div>
		
		</div>
		<br class="clearfix" />
	</div>
    
</div>
<%include footer%>
