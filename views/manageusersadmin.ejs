<%include head %>
<%include adminnavigator%>

    <div id="page">
<center>
<h2>View All Register Users List</h2>

<table border='2' cellspacing='10'>
<tr align='center'>
 <th>RegID</th>
 <th>Name</th>
 <th>Email</th>
 <th>Address</th>
 <th>City</th>
 <th>Mobile</th>
 <th>Gender</th>
 <th>Block/Unblock</th>
 <th>Action</th>					
</tr>

<%
for(i=0;i<data.length;i++) {
%>
<tr align='center'>
<td><%= data[i].regid %></td>
<td><%= data[i].nm %></td>
<td><%= data[i].email %></td>
<td><%= data[i].address %></td>
<td><%= data[i].city %></td>
<td><%= data[i].mobile %></td>
<td><%= data[i].gender %></td>
<% if(data[i].vstatus=='1') { %>
<td>
<a href='/admin/validateusers?block=<%= data[i].regid %>' title='Block Users'>
<img src='../images/block.jpeg' height='50' width='50'/>
</a>
</td>
<% } else { %>
<td>
<a href='/admin/validateusers?unblock=<%= data[i].regid %>' title='Un-Block Users'>
<img src='../images/unblock.png' height='50' width='50'/>
</a>
</td>
<% } %>

<td>
<a href='/admin/validateusers?delete=<%= data[i].regid %>' title='Delete Users'>
<button><img src='../images/delete.jpeg' height='50' width='70' /></button>
</a>
</td>

</tr>
<% } %>

</table>
<center>
    
</div>
<%include footer%>
